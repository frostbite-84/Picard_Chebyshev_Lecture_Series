\[
a_{\alpha}=\frac{1}{c_{\alpha}}\left\{ \overset{M}{\underset{j=0}{\sum}}W_{j}T_{\alpha}(\xi_{j})f(\xi_{j})\right\} 
\]

\end_inset


\begin_inset Formula 
\begin{equation}
=\frac{1}{c_{\alpha}}\left\{ \frac{1}{2}T_{\alpha}(\xi_{0})f(\xi_{0})+...+T_{\alpha}(\xi_{M-1})f(\xi_{M-1})+\frac{1}{2}T_{\alpha}(\xi_{M})f(\xi_{M})\right\} ,\label{eq:P2_Eq13}
\end{equation}

