\begin_inset Formula 
\begin{equation}
c_{\alpha}=\overset{M}{\underset{j=0}{\sum}}W_{j}T_{\alpha}^{2}(\xi_{j})=\left\{ \frac{1}{2}T_{\alpha}^{2}(\xi_{0})+T_{\alpha}^{2}(\xi_{1})+...+T_{\alpha}^{2}(\xi_{M-1})+\frac{1}{2}T_{\alpha}^{2}(\xi_{M})\right\} ,\:\alpha=0,1,...,N,\label{eq:P2_Eq14}
\end{equation}

\end_inset


\begin_inset Formula 
\begin{equation}
\left.\begin{array}{ccc}
c_{0} & =\langle T_{0}(\xi),T_{0}(\xi)\rangle=M\\
c_{\alpha} & =\langle T_{\alpha}(\xi),T_{\alpha}(\xi)\rangle=M/2, & \alpha=1,2,...,N-1\\
c_{_{N}} & =\langle T_{N}(\xi),T_{N}(\xi)\rangle=M, & \mathrm{if}\:M=N\:(\mathrm{interpolation\:case})\\
c_{_{N}} & =\langle T_{N}(\xi),T_{N}(\xi)\rangle=M/2, & \mathrm{if}\:M>N\:(\mathrm{least\:squares\:case})
\end{array}\right\} .\label{eq:P1_Eq15}
\end{equation}

